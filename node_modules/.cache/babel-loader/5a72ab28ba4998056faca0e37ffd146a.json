A,CADA,GAEA,UAAA,CAAA,OAAA,EAAA,CAAA,YAAA,EAAA,YAAA,CAAA,CAFA;AAGA,IAAA,cAHA,4BAGA;AACA,UAAA,KAAA,QAAA,EAAA;AACA,aAAA,UAAA,CAAA;AAAA,UAAA,EAAA,EAAA,KAAA,IAAA,CAAA,EAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA,SAAA;AACA,aAAA,QAAA,GAAA,IAAA;AACA,aAAA,QAAA,CAAA;AAAA,UAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,SAAA;AACA,OAJA,MAIA;AACA,aAAA,UAAA,CAAA;AAAA,UAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,SAAA;AACA;AACA,KAXA;AAYA,IAAA,cAZA,4BAYA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA,KAhBA;AAiBA,IAAA,iBAjBA,+BAiBA;AACA,WAAA,eAAA,GAAA,CAAA,KAAA,eAAA;AACA,KAnBA;AAoBA,IAAA,iBApBA,+BAoBA;AACA,WAAA,eAAA,GAAA,CAAA,KAAA,eAAA;AACA,WAAA,UAAA,CAAA;AAAA,QAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,OAAA;AACA;AAvBA,IAtBA;AA+CA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,oBACA,KADA,EACA;AACA,UAAA,CAAA,KAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA;AACA,KALA;AAMA,IAAA,IAAA,EAAA;AACA,MAAA,SAAA,EAAA,IADA;AAEA,MAAA,OAFA,qBAEA;AAAA;;AACA,aAAA,QAAA,GAAA,CAAA,KAAA,IAAA,CAAA,QAAA;AACA,aAAA,QAAA,GAAA,KAAA,IAAA,CAAA,IAAA;;AACA,YAAA,KAAA,IAAA,CAAA,QAAA,EAAA;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA,WAFA,EAEA,GAFA,CAAA;AAGA;AACA;AAVA;AANA,GA/CA;AAkEA,EAAA,OAlEA,qBAkEA,CAAA;AAlEA,CAAA","sourcesContent":["<template>\n  <div :class=\"['file-item', { active: isActive }]\">\n    <div\n      class=\"clickable-area\"\n      @click=\"openFile({ id: file.id })\"\n      @dblclick=\"readonly = !readonly\"\n    >\n      <FileTextIcon class=\"icon\" size=\"18\" />\n      <form @submit.prevent=\"$refs.input.blur()\">\n        <input\n          :ref=\"'input'\"\n          type=\"text\"\n          v-model=\"filename\"\n          :readonly=\"readonly\"\n          size=\"2\"\n          @blur=\"changeFileName\"\n        />\n      </form>\n    </div>\n    <div class=\"context-menu\">\n      <MoreVerticalIcon\n        class=\"trigger-icon\"\n        size=\"18\"\n        @click=\"toggleContextMenu\"\n      />\n      <SlideYUpTransition>\n        <div\n          v-if=\"showContextMenu\"\n          class=\"options\"\n          v-click-outside=\"toggleContextMenu\"\n        >\n          <div class=\"option-item\" @click=\"openRenameMode\">\n            <edit3-icon size=\"18\" class=\"icon\" />Rename\n          </div>\n          <!-- <div class=\"option-item\">\n            <download-icon size=\"18\" class=\"icon\" />Download File\n          </div>\n          <div class=\"option-item\">\n            <copy-icon size=\"18\" class=\"icon\" />Duplicate File\n          </div>\n          <div class=\"option-item\">\n            <clipboard-icon size=\"18\" class=\"icon\" />Copy contents\n          </div> -->\n          <div class=\"option-item\" @click=\"deleteCurrentFile\">\n            <trash2-icon size=\"18\" class=\"icon\" />Delete File\n          </div>\n        </div>\n      </SlideYUpTransition>\n    </div>\n    <!-- <span>{{ file.name }}</span> -->\n  </div>\n</template>\n\n<script>\nimport {\n  FileTextIcon,\n  MoreVerticalIcon,\n  Trash2Icon,\n  Edit3Icon,\n  DownloadIcon,\n  CopyIcon,\n  ClipboardIcon,\n} from \"vue-feather-icons\";\nimport { mapActions } from \"vuex\";\nimport { SlideYUpTransition } from \"vue2-transitions\";\n\nexport default {\n  components: {\n    FileTextIcon,\n    MoreVerticalIcon,\n    Trash2Icon,\n    Edit3Icon,\n    DownloadIcon,\n    CopyIcon,\n    ClipboardIcon,\n    SlideYUpTransition,\n  },\n  props: {\n    file: Object,\n    isActive: Boolean,\n  },\n  data() {\n    return {\n      readonly: true,\n      filename: \"\",\n      showContextMenu: false,\n    };\n  },\n  methods: {\n    ...mapActions(\"Editor\", [\"openFile\"]),\n    ...mapActions(\"Files\", [\"renameFile\", \"deleteFile\"]),\n    changeFileName() {\n      if (this.filename) {\n        this.renameFile({ id: this.file.id, name: this.filename });\n        this.readonly = true;\n        this.openFile({ id: this.file.id });\n      } else {\n        this.deleteFile({ id: this.file.id });\n      }\n    },\n    openRenameMode() {\n      this.showContextMenu = false;\n      this.readonly = false;\n      this.$refs.input.focus();\n    },\n    toggleContextMenu() {\n      this.showContextMenu = !this.showContextMenu;\n    },\n    deleteCurrentFile() {\n      this.showContextMenu = !this.showContextMenu;\n      this.deleteFile({ id: this.file.id });\n    },\n  },\n  watch: {\n    readonly(value) {\n      if (!value) {\n        this.$refs.input.focus();\n      }\n    },\n    file: {\n      immediate: true,\n      handler() {\n        this.readonly = !this.file.editable;\n        this.filename = this.file.name;\n        if (this.file.editable) {\n          setTimeout(() => {\n            this.$refs.input.focus();\n          }, 200)\n        }\n      },\n    },\n  },\n  mounted() {},\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.file-item {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  padding: 2px 5px 2px 0;\n  margin: 2px 5px;\n  border-radius: 5px;\n\n  &.active {\n    // color: var(--color-primary);\n    background: var(--color-secondary);\n  }\n\n  .clickable-area {\n    display: flex;\n    align-items: center;\n    flex: 1;\n    padding: 3px 10px;\n  }\n  .icon {\n    margin-right: 5px;\n    width: 20px;\n  }\n\n  form {\n    flex: 1;\n    display: flex;\n    flex-direction: column;\n  }\n\n  input {\n    border: 0;\n    padding: 6px 8px;\n    border-radius: 2px;\n    color: var(--font-color);\n    border: 2px solid transparent;\n    outline: none;\n    transition: 0.2s all ease-in-out;\n    background: transparent;\n    min-width: 0;\n\n    &[readonly] {\n      pointer-events: none;\n    }\n\n    &:focus {\n      border-bottom: 2px solid var(--color-primary);\n    }\n  }\n\n  .context-menu {\n    display: none;\n    align-items: center;\n    justify-content: center;\n    align-items: center;\n    position: relative;\n    transition: 0.3s all ease-in-out;\n\n    .trigger-icon {\n      visibility: hidden;\n      padding: 5px;\n      border-radius: 5px;\n      &:hover {\n        background: var(--color-secondary-light);\n      }\n\n      &:active {\n        transform: scale(0.95);\n      }\n    }\n\n    .options {\n      position: absolute;\n      right: 0;\n      top: 35px;\n      width: 170px;\n      height: auto;\n      z-index: 99;\n      display: flex;\n      flex-direction: column;\n      border-radius: 5px;\n      background: var(--color-secondary);\n      box-shadow: var(--smooth-shadow);\n      border: 1px solid var(--border-color);\n      padding: 5px;\n\n      .option-item {\n        display: flex;\n        align-items: center;\n        padding: 10px;\n        border-radius: 3px;\n\n        &:hover {\n          cursor: pointer;\n          // background: var(--color-secondary-light);\n          color: var(--color-primary);\n        }\n      }\n    }\n  }\n\n  &:hover {\n    background: var(--color-secondary);\n    cursor: pointer;\n    color: var(--font-color);\n\n    .context-menu {\n      display: flex;\n      .trigger-icon {\n        visibility: visible;\n      }\n    }\n  }\n\n  // &:read-only {\n  //   background: transparent;\n  // }\n}\n</style>\n"],"sourceRoot":"src/components"}]}